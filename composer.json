{
    "name": "phpactor/behat-extension",
    "description": "Behat Support",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "prefer-stable":true,
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/container": "^1.3.4",
        "phpactor/completion": "^0.4.3",
        "phpactor/file-path-resolver-extension": "^0.3.4",
        "phpactor/worse-reflection-extension": "^0.2.4",
        "behat/gherkin": "^4.5",
        "behat/behat": "^3.5",
        "phpactor/completion-extension": "^0.2.4",
        "symfony/yaml": "^4.3 || ^5.1",
        "phpactor/reference-finder-extension": "^0.1.7",
        "php": "^7.3 || ^8.0",
        "dms/phpunit-arraysubset-asserts": "^0.2.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpactor/test-utils": "^1.1.3",
        "rector/rector": "0.8.52",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\Behat\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\Behat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\Behat\\BehatExtension",
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
